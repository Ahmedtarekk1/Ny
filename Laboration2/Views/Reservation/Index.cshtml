@model IEnumerable<Lan>

@{
    ViewData["Title"] = "All Reservations";
}
<link rel="stylesheet" href="~/css/API.css">

<h2>Län och dess Invånare</h2>


<table class="table table-sm table-striped table-bordered m-2">
    <thead>
        <tr>
            <th>ID</th>
            <th>LänNamn</th>
            <th>Antal Invånare</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var r in Model)
        {
            <tr>
                <td>@r.Id</td>
                <td>@r.LanNamn</td>
                <td>@r.AntalInvanare</td>
                
             
            </tr>
        }
    </tbody>
</table>